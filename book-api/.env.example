# === Server Settings ===
PORT=3000
NODE_ENV=development

# === MongoDB ===
# Replace with your actual connection string in .env (do not commit real values)
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<dbname>

# === Auth ===
# Use a 64-byte hex string generated with: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=your_64_byte_hex_secret_here

# === Frontend Origin(s) ===
# Comma-separated list of allowed origins (no spaces)
CLIENT_URL=http://localhost:5173,https://your-frontend.vercel.app
